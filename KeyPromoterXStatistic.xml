<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide Active Tool Window">
        <Item>
          <IdeaActionID>HideActiveWindow</IdeaActionID>
          <ShortCut>'Shift+Esc'</ShortCut>
          <Description>Hide Active Tool Window</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Last Tool Window">
        <Item>
          <IdeaActionID>JumpToLastWindow</IdeaActionID>
          <ShortCut>'F12'</ShortCut>
          <Description>Jump to Last Tool Window</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Folder...">
        <Item>
          <IdeaActionID>FileChooser.NewFolder</IdeaActionID>
          <ShortCut>'Alt+Insert' or 'Ctrl+N'</ShortCut>
          <Description>New Folder...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'Ctrl+Shift+K'</ShortCut>
          <Description>Push...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Default Layout">
        <Item>
          <IdeaActionID>RestoreDefaultLayout</IdeaActionID>
          <ShortCut>'Shift+F12'</ShortCut>
          <Description>Restore Default Layout</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run...">
        <Item>
          <IdeaActionID>ChooseRunConfiguration</IdeaActionID>
          <ShortCut>'Alt+Shift+F10'</ShortCut>
          <Description>Run...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diagram Popup...">
        <Item>
          <IdeaActionID>ShowUmlDiagramPopup</IdeaActionID>
          <ShortCut>'Ctrl+Alt+U'</ShortCut>
          <Description>Show Diagram Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diagram...">
        <Item>
          <IdeaActionID>ShowUmlDiagram</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+U'</ShortCut>
          <Description>Show Diagram...</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="VCS Operations Popup...">
        <Item>
          <IdeaActionID>Vcs.QuickListPopupAction</IdeaActionID>
          <ShortCut>'Alt+`'</ShortCut>
          <Description>VCS Operations Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Create Graph">
        <Item>
          <IdeaActionID>CodeIris.ReadModule</IdeaActionID>
          <Description>Create Graph</Description>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reload PlantUml Diagrams">
        <Item>
          <IdeaActionID>PlantUML.Reload</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+G'</ShortCut>
          <Description>Reload PlantUml Diagrams</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update PlantUml Diagrams - changes only">
        <Item>
          <IdeaActionID>PlantUML.Update</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+F'</ShortCut>
          <Description>Update PlantUml Diagrams - changes only</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>